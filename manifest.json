{
	"name": "Session Bookmarker",
	"description": "Saves the URL of all tabs in current window for easy revisits.",
	"version": "1.0",
	"permissions": ["tabs"],
	"action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "images/16.png",
		"32": "images/32.png",
		"48": "images/48.png",
		"128": "images/128.png"
	},
	"manifest_version": 3
}